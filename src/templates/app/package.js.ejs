{
  "name": "<%= props.name %>",
  "version": "0.0.1",
  "scripts": {
    "build": "npm run cpviews && node_modules/.bin/tsc",
    "start": "node dist/main.js",
    "serve": "NODE_ENV=desenv nodemon -L dist/main.js",
    "test": "NODE_ENV=test ts-node jest.startup.ts --detectOpenHandles --forceExit --no-cache",
    "test:file": "npm test --",
    "notify:event": "node dist/jobs/notify/event.js",
    "cpviews": "cp -a views dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com//<%= props.name %>.git"
  },
  "bugs": {
    "url": "https://github.com//<%= props.name %>/issues"
  },
  "homepage": "https://github.com//<%= props.name %>#readme",
  "jest": {
    "globals": {
      "address": "http://localhost:3001"
    },
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "accept-language": "^3.0.18",
    "assert-plus": "^1.0.0",
    "bluebird": "^3.5.4",
    "i18next": "^15.1.2",
    "install": "^0.12.2",
    "joi": "^13.7.0",
    "joi-objectid": "^2.0.0",
    "mongoose": "^5.5.10",
    "mongoose-timestamp": "^0.6.0",
    "node-jsencrypt": "^1.0.0",
    "node-schedule": "^1.3.2",
    "npm": "^6.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "redis": "^2.8.0",
    "restify": "^7.7.0",
    "restify-cors-middleware": "^1.1.1",
    "restify-errors": "^6.1.1",
    "restify-joi-middleware": "^5.0.1",
    "restify-render-middleware": "^2.0.1",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@types/assert-plus": "^1.0.4",
    "@types/faker": "^4.1.5",
    "@types/i18next": "^12.1.0",
    "@types/jest": "^23.3.14",
    "@types/joi": "^13.6.3",
    "@types/mongoose": "^5.5.1",
    "@types/node-schedule": "^1.2.3",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/redis": "^2.8.12",
    "@types/restify": "^7.2.10",
    "@types/restify-cors-middleware": "^1.0.1",
    "@types/restify-errors": "^4.3.3",
    "@types/socket.io": "^2.1.1",
    "@types/supertest": "^2.0.7",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "nodemon": "^1.19.0",
    "pre-commit": "^1.2.2",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.4.5"
  },
  "pre-commit": [
    "test"
  ]
}
